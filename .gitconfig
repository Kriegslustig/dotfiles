[user]
	name = Kriegslustig
	email = github@ls7.ch
[push]
	default = simple
[core]
	editor = /usr/bin/vim
	excludesfile = /Users/kriegslustig/.gitignore_global
[global]
	excludesfile = ~/.gitignore


[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
